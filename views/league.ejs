<style>
.table_style{
    margin-top: 10px;
    margin-left: 150px;
}
.c1{ color: rgba(180,210,170,0.67)}
.c2{ color: #1b1e21}
.c3{ color: rgba(59,58,210,0.67)}
.c4{ color: rgba(22,135,15,0.67) }
.c5{}
.c6{ color: rgba(255,5,5,0.67)}
.c7{}
.c8{}
.c9{}
.c10{}

    th, td{ margin: 10px;
    padding:15px 25px 0 20px; text-align: center}
    h1{text-align: center; font-size: xx-large; font-weight: bolder}

</style>

<div class="row table_style">
    <h1>Premier League Table</h1>
    <br/>
    <div class="col-md-2">
<table border="0">
    <tr>
        <th></th>
        <th>Team</th>
        <th>Played</th>
        <th>Won</th>
        <th>Drawn</th>
        <th>Lost</th>
        <th>Goals</th>
        <th>GA</th>
        <th>GD</th>
        <th>Points</th>

    </tr>
    <% data.standing.forEach(function(item) { %>
    <tr>
        <td class="c1"><%= item.position%></td>
        <td class="c2"style="font-weight: bolder; padding-right: 100px"><%= item.teamName %></td>
        <td class="c3"> <%= item.playedGames %></td>
        <td class="c4"><%= item.wins %></td>
        <td class="c5"><%= item.draws %></td>
        <td class="c6"><%= item.losses %></td>
        <td class="c7"><%= item.goals %></td>
        <td class="c8"><%= item.goalsAgainst %></td>
        <td class="c9"><%= item.goalDifference %></td>
        <td class="c10"><%= item.points %></td>
    </tr>
    <% }); %>
</table>
    </div>
</div>
<br/><br/><br/>